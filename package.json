{
  "name": "simple-standup-bot",
  "version": "0.1.1",
  "description": "Dead Simple Standup Bot for Slack",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "ports": "lt --port 3000 & lt --port 3001",
    "clean": "turbo daemon clean && rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "build": "turbo run build",
    "start": "turbo run start",
    "production": "turbo run production",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate": "turbo run migrate",
    "db:generate": "turbo run generate",
    "docker:build": "./build.sh",
    "docker:start": "./start.sh"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.13.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.0",
    "node-schedule": "^2.1.1",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@turbo/gen": "^1.10.6",
    "drizzle-kit": "^0.19.2",
    "eslint": "^8.43.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.10.12"
  }
}
