{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "start": {},
    "production": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false
    },
    "generate": {
      "cache": false
    }
  }
}
